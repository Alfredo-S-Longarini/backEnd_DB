{{#if productos.length}}
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Stock</th>
            <th scope="col">Imagen</th>
        </tr>
    </thead>
    {{#each productos}}
        <tbody style="line-height: 50px;">
            <td>{{this.nombre}}</td>
            <td>${{this.precio}}</td>
            <td>${{this.stock}}</td>
            <td><img style="width: 10%;" src={{this.img}} alt=""></td>
        </tbody>
    {{/each}}
</table>
{{else}}
<h4>No hay productos en la lista</h4>
{{/if}}
